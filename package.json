{
  "name": "cosmical-space-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "dev:api": "NODE_ENV=development nodemon ./server/src/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "screenshots": "node scripts/generate-screenshots.js"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.4.7",
    "pinia": "^2.0.33",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.14",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cypress": "^13.17.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^22.1.0",
    "mongodb": "^6.12.0",
    "msw": "^2.7.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^0.14.7",
    "vitest": "^2.1.8"
  }
}
